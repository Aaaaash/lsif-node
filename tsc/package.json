{
	"name": "lsif-tsc",
	"version": "0.0.2",
	"description": "Tool to create an LSIF dump for TypeScript projects.",
	"main": "lib/main.js",
	"typings": "lib/main.d.ts",
	"bin": {
		"lsif-tsc": "./bin/lsif-tsc"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Microsoft/vscode-typescript-sip.git"
	},
	"author": "MS",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Microsoft/vscode-typescript-sip/issues"
	},
	"homepage": "https://github.com/Microsoft/vscode-typescript-sip#readme",
	"dependencies": {
		"minimist": "^1.2.0",
		"typescript": "^3.3.4000",
		"uuid": "^3.3.2",
		"vscode-uri": "^1.0.6",
		"lsif-protocol": "0.2.0"
	},
	"devDependencies": {
		"@types/minimist": "^1.2.0",
		"@types/node": "^10.14.3",
		"@types/uuid": "^3.4.4"
	},
	"scripts": {
		"compile": "node ../build/bin/tsc -b ./tsconfig.json",
		"compile:prod": "node ../build/bin/tsc -b ./tsconfig.prod.json",
		"watch": "node ../build/bin/tsc -w -b ./tsconfig.json",
		"clean": "node ../node_modules/rimraf/bin.js lib",
		"prepublishOnly": "npm run clean && npm run compile:prod",
		"postpublish": "node ../build/bin/post-publish.js"
	}
}
